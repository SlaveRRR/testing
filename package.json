{
   "name": "testing",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "dev": "node index.js",
      "test": "mocha tests/*.js --recursive --timeout 60000",
      "allure": "npx allure serve allure-results"
   },
   "dependencies": {
      "chai": "^4.3.7",
      "mocha": "^10.4.0",
      "selenium-webdriver": "^4.19.0"
   },
   "devDependencies": {
      "allure-commandline": "^2.29.0",
      "allure-mocha": "^2.15.1"
   },
   "mocha": {
      "parallel": false,
      "reporter": "allure-mocha",
      "reporterOptions": {
         "resultsDir": "allure-results"
      }
   }
}
